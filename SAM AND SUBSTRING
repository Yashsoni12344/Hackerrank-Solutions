int substrings(string n) {
    long long prev = n[0] - '0';
    long long ans = prev;

    for (int i = 1; i < n.size(); i++) {
        long long d = n[i] - '0';
        long long curr = (prev * 10 + (i + 1) * d) % MOD;
        ans = (ans + curr) % MOD;
        prev = curr;
    }

    return ans % MOD;
}
